<h2>Log In</h2>
<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs deviseForm">
    <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { autocomplete: "email" }, class:"deviseText" %>
    <%= f.input :password,
                required: false,
                input_html: { autocomplete: "current-password" } %>

  </div>
  
  <div class="form-actions deviseForm">
    <%= f.button :submit, "Log in" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
